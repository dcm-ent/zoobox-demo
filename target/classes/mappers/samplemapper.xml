<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.dcm.zoobox.mapper.SampleMapper" >
		<select id="getList" parameterType="int" resultType="Actor">
			SELECT actor_id actorId, first_name firstName, last_name lastName
			FROM sakila.actor
			limit #{value}
		</select>
</mapper>