<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.dcm.zoobox.place.mapper.PlaceMapper">
	<insert id="save" parameterType="java.util.ArrayList">
		insert into workplace
		(workplace_name,workplace_busi_name,workplace_busi_phone,workplace_busi_address)
		values
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.workPlaceName},#{item.busiName},#{item.busiPhone},#{item.busiAddress})
		</foreach>
	</insert>
</mapper>