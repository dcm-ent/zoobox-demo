<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
 
<configuration>
	
	<settings> 
		<setting name="mapUnderscoreToCamelCase" value="true"/> 
	</settings> 
	
	<typeAliases>
		<typeAlias alias="User" type="com.dcm.zoobox.user.model.User"/>
		<typeAlias alias="UserInfo" type="com.dcm.zoobox.user.model.UserInfo"/>
		<typeAlias alias="UserRole" type="com.dcm.zoobox.user.model.UserRole"/>
		<typeAlias alias="UserSnsInfo" type="com.dcm.zoobox.user.model.UserSnsInfo"/>
		<typeAlias alias="PetBreed" type="com.dcm.zoobox.user.model.PetBreed"/>
		<typeAlias alias="PetCategory" type="com.dcm.zoobox.user.model.PetCategory"/>
		<typeAlias alias="Place" type="com.dcm.zoobox.place.model.WishListPlace"/>
		<typeAlias alias="Main" type="com.dcm.zoobox.main.model.Main"/>
		<typeAlias alias="Board" type="com.dcm.zoobox.community.model.Board"/>
		<typeAlias alias="Article" type="com.dcm.zoobox.community.model.Article"/>
		<typeAlias alias="Comment" type="com.dcm.zoobox.community.model.Comment"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/usermapper.xml"/>
		<mapper resource="mappers/placemapper.xml"/>
		<mapper resource="mappers/mainmapper.xml"/>
		<mapper resource="mappers/boardmapper.xml"/>
		<mapper resource="mappers/articlemapper.xml"/>
		<mapper resource="mappers/commentmapper.xml"/>
	</mappers>
	
</configuration>